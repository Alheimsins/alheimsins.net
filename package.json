{
  "name": "alheimsins.net",
  "version": "1.0.2",
  "description": "alheimsins.net website",
  "main": "index.js",
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "deploy": "npm test && now && now alias",
    "export": "next build && next export",
    "now-build": "next build && next export",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alheimsins/alheimsins.net.git"
  },
  "keywords": [
    "alheimsins"
  ],
  "author": {
    "name": "Jonas Enge",
    "email": "jonas.enge@gmail.com",
    "url": "https://github.com/maccyber"
  },
  "contributors": [
    {
      "name": "Geir GÃ¥sodden",
      "email": "geir.gasodden@pythonia.no",
      "url": "https://github.com/zrrrzzt"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alheimsins/alheimsins.net/issues"
  },
  "homepage": "https://github.com/alheimsins/alheimsins.net#readme",
  "dependencies": {
    "axios": "0.19.2",
    "next": "9.2.2",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "ava": "3.3.0",
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.9",
    "nyc": "15.0.0",
    "standard": "14.3.1"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "engines": {
    "node": ">=10.15.1"
  }
}
