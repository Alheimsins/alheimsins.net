{
  "name": "alheimsins.net",
  "version": "1.0.0",
  "description": "alheimsins.net website",
  "main": "index.js",
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "gather-data": "node gather-data.js",
    "build": "next build",
    "start": "next start",
    "deploy": "npm run gather-data && npm test && next build && next export && now --static out/ --name alheimsins.net && now alias",
    "export": "next build && next export"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alheimsins/alheimsins.net.git"
  },
